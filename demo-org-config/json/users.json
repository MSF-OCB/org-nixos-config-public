{
  "global_admins": ["global_admin_1", "global_admin_2", "global_admin_3"],
  "users": {
    "per-host": {
      "demo-host": {
        "enable_roles": ["devops_dev"]
      },
      "demo-host-2": {
        "enable_roles": ["hq", "infra_hq"],
        "enable_roles_with_profile": {
          "devops": "admin",
          "fieldSupport": "admin"
        }
      },
      "demo-host-3": {
        "enable_roles": ["devops", "infra_hq"]
      },
      "demo-host-4": {
        "enable_roles": ["extra_relay_access"]
      }
    },
    "roles": {
      "emr_support": {
        "enable": {
          "doctor_fictional": "dockerAdmin"
        }
      },
      "devops": {
        "enable_roles": ["devops_common"]
      },
      "devops_common": {
        "enable": {
          "global_admin_1": "admin",
          "global_admin_2": "admin",
          "global_admin_3": "admin"
        }
      },
      "devops_docker_hq": {
        "enable_roles": ["devops_common"]
      },
      "dhis2_support": {
        "enable_roles": ["devops"]
      }
    }
  }
}
