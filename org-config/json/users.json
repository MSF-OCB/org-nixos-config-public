{
  "global_admins": ["global_admin_1", "global_admin_2", "global_admin_3"],
  "users": {
    "per-host": {
      "demo-test-host": {
        "enable_roles": ["devops"]
      },
      "demo-prod-host": {
        "enable_roles": ["devops_common", "emr_support"],
        "enable_roles_with_profile": {
          "devops": "admin",
          "fieldSupport": "admin"
        }
      },
      "demo-uat-host": {
        "enable_roles": ["devops", "emr_support"]
      },
      "demo-relay-1": {
        "enable_roles": ["extra_relay_access"]
      },
      "demo-relay-2": {
        "enable_roles": ["extra_relay_access"]
      }
    },
    "roles": {
      "emr_support": {
        "enable": {
          "doctor_fictional": "dockerAdmin"
        }
      },
      "fieldSupport": {
        "enable": {
          "doctor_fictional": "admin"
        }
      },
      "extra_relay_access": {
        "enable": {
          "doctor_fictional": "dockerAdmin"
        }
      },
      "devops": {
        "enable_roles": ["devops_common"]
      },
      "devops_common": {
        "enable": {
          "global_admin_1": "admin",
          "global_admin_2": "admin",
          "global_admin_3": "admin"
        }
      },
      "devops_docker_hq": {
        "enable_roles": ["devops_common"]
      },
      "dhis2_support": {
        "enable_roles": ["devops"]
      }
    }
  }
